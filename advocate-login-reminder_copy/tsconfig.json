{
  "compilerOptions": {
    "lib": [
      "es2018",
      "es2017",
      "es2015",
      "esnext.asynciterable"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "lib",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules"
  ]
}
