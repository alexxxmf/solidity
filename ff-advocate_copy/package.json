{
  "name": "digitalmentor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test-watch": "jest -o --watch",
    "run-ios": "react-native run-ios",
    "run-android": "react-native run-android",
    "run-link": "RNFB_ANDROID_PERMISSIONS=true react-native link",
    "tsc": "tsc",
    "bump-ios": "cd ios && fastlane ios bump_build && cd ..",
    "bump-ios-patch": "cd ios && fastlane ios bump_patch && cd ..",
    "bump-android:no-push": "cd android && fastlane android bump_version_code && cd ..",
    "bump-android": "yarn bump-android:no-push && git push",
    "clear-cache": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && yarn cache clean --force && yarn",
    "clear-bundler": "watchman watch-del-all && rm -rf node_modules && yarn install && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "lint": "eslint --ext \"ts,tsx,js,jsx\" --fix src",
    "build:js:ios": "react-native bundle --platform ios --entry-file index.js --bundle-output ios.jsbundle",
    "build:js:android": "react-native bundle --platform android --entry-file index.js --bundle-output android.jsbundle",
    "ensure_everything_is_linked": "react-native link && chmod +x ./ensure_git_status_is_clean.sh && ./ensure_git_status_is_clean.sh",
    "build:js:ios:map": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output main.jsbundle --sourcemap-output sourcemap.ios.js.map --sourcemap-sources-root ./",
    "build:js:android:map": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output main.jsbundle --sourcemap-output sourcemap.android.js.map --sourcemap-sources-root ./",
    "e2e:ios:build": "ENVFILE=.env.e2e detox build -c ios.sim.debug",
    "e2e:ios:test": "JEST_JUNIT_OUTPUT=\"./artifacts/test-results/ios/junit.xml\" ENVFILE=.env.e2e detox test -c ios.sim.debug --artifacts-location ./artifacts/ios/ --record-videos all",
    "e2e:ipad:build": "ENVFILE=.env.e2e detox build -c ios.ipadsim.debug ",
    "e2e:ipad:test": "ENVFILE=.env.e2e detox test -c ios.ipadsim.debug --artifacts-location ./artifacts/ipad/ --record-videos all",
    "e2e:android:build": "ENVFILE=.env.e2e detox build -c android.emu.debug",
    "e2e:android:test": "ENVFILE=.env.e2e detox test -c android.emu.debug  --artifacts-location ./artifacts/android/ --record-videos all"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.2.4",
    "apollo-cache-inmemory": "^1.3.5",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-retry": "^2.2.10",
    "aws-amplify": "^1.1.19",
    "aws-amplify-react-native": "^2.1.7",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "formik": "^1.3.2",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.11",
    "lottie-react-native": "^2.6.1",
    "moment": "^2.24.0",
    "prop-types": "^15.6.0",
    "react": "16.8.3",
    "react-apollo": "^2.4.1",
    "react-countdown-now": "^2.1.0",
    "react-native": "0.59.8",
    "react-native-branch": "2.3.3",
    "react-native-config": "^0.11.7",
    "react-native-device-info": "^2.1.2",
    "react-native-elements": "^1.0.0",
    "react-native-email-link": "^1.2.0",
    "react-native-intercom": "^12.2.0",
    "react-native-keep-awake": "^4.0.0",
    "react-native-offline": "^4.3.1",
    "react-native-restart": "^0.0.8",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-splash-screen": "3.1.1",
    "react-native-uuid-generator": "^5.0.0",
    "react-native-vector-icons": "6.1.0",
    "react-native-webview": "^5.2.1",
    "react-native-wheel-pick": "^1.0.9",
    "react-navigation": "^2.17.0",
    "rollbar-react-native": "^0.5.0",
    "rxjs": "^6.3.3",
    "styled-components": "^4.2.0",
    "unfetch": "^4.1.0",
    "urijs": "^1.19.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.121",
    "@types/node-fetch": "^2.1.6",
    "@types/react": "^16.8.14",
    "@types/react-native": "^0.57.51",
    "@types/react-native-keep-awake": "^2.0.2",
    "@types/react-native-restart": "^0.0.0",
    "@types/react-native-snap-carousel": "^3.7.1",
    "@types/react-native-uuid-generator": "^4.0.0",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "^3.0.7",
    "@types/react-test-renderer": "^16.8.1",
    "@types/styled-components": "^4.1.14",
    "@types/yup": "^0.26.12",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "detox": "^12.10.2",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "1.2.0",
    "jest": "^24.8.0",
    "jest-environment-jsdom": "^24.8.0",
    "jest-junit": "^6.4.0",
    "lint-staged": "8.1.3",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.16.4",
    "react-native-testing-library": "^1.8.0",
    "react-test-renderer": "16.8.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/digitalmentor/Build/Products/Debug-iphonesimulator/digitalmentor.app",
        "build": "xcodebuild -project ios/digitalmentor.xcodeproj -scheme digitalmentor -configuration Debug -destination 'platform=iOS Simulator,name=iPhone X' -derivedDataPath ios/build/digitalmentor",
        "type": "ios.simulator",
        "name": "iPhone X"
      },
      "ios.ipadsim.debug": {
        "binaryPath": "ios/build/digitalmentor/Build/Products/Debug-iphonesimulator/digitalmentor.app",
        "build": "xcodebuild -project ios/digitalmentor.xcodeproj -scheme digitalmentor -configuration Debug -destination 'platform=iOS Simulator,name=iPad Pro (12.9-inch) (3rd generation)' -derivedDataPath ios/build/digitalmentor",
        "type": "ios.simulator",
        "name": "iPad Pro (12.9-inch) (3rd generation)"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Pixel_3_API_28"
      }
    },
    "test-runner": "jest"
  }
}
